{
  "name": "langchain-study",
  "version": "1.0.0",
  "description": "disect how langchain ReAct prompt cycle works",
  "exports": "./src/index.ts",
  "type": "module",
  "scripts": {
    "start": "tsx --env-file=.env.local src/index.ts",
    "build": "tsc -b",
    "dev": "tsx --env-file=.env.local --watch ./src/index.ts",
    "server": "tsx --env-file=.env.local --watch ./server/server.ts",
    "format": "npm run eslint -- --fix && npm run prettier -- --write",
    "lint": "npm run eslint && npm run prettier && tsc --noEmit",
    "prettier": "prettier --config .prettierrc.json '**/*.{css,g?(raph)ql,js,json,jsx,html,md,less,sass,scss,ts,tsx,y?(a)ml}' --check",
    "eslint": "eslint . --cache --cache-location 'node_modules/.cache/eslint/' --max-warnings=0 --report-unused-disable-directives ."
  },
  "author": "Dan Stein",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/cors": "^2.8.17",
    "@types/node": "^22.13.14",
    "@types/node-fetch": "^2.6.12",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "10.1.1",
    "globals": "^16.0.0",
    "langchain": "^0.3.19",
    "neo4j-driver": "^5.28.1",
    "prettier": "3.5.0",
    "tsx": "^4.19.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0"
  },
  "dependencies": {
    "@apollo/server": "^4.11.3",
    "@neo4j/graphql": "^6.5.3",
    "@neo4j/introspector": "^4.0.1",
    "chalk": "^5.4.1",
    "expr-eval": "^2.0.2",
    "express": "^4.21.2",
    "graphql": "^16.10.0",
    "graphql-request": "^7.1.2",
    "graphql-tag": "^2.12.6",
    "mongoose": "^8.13.0",
    "node-nlp": "^5.0.0-alpha.5",
    "ollama": "^0.5.14",
    "puppeteer": "^24.4.0",
    "serpapi": "^2.1.0",
    "type-graphql": "^2.0.0-rc.2"
  }
}
